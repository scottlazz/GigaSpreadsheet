{
  "name": "gigasheet",
  "private": false,
  "version": "0.0.1",
  "source": "src/index.html",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/indexejs.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sheet": {
      "import": "./dist/sheet/index.js",
      "require": "./dist/sheet/index.js",
      "types": "./dist/sheet/index.d.ts"
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build:ts": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build:lib": "npm run build:ts",
    "prepare": "npm run build:ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest tests/sheet-integration.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "gh-pages": "^6.3.0",
    "golden-layout": "^2.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.16.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  }
}
